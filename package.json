  "scripts": {
    "build": "tsc",
    "test": "jest --coverage"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "express": "^4.16.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "universal-router": "^5.0.0"
  },
  "devDependencies": {
    "@types/colors": "^1.1.3",
